  </ItemGroup>
  <ItemGroup>
    <Folder Include="Services\" />
    <Folder Include="Services\Implementation\" />
    <Folder Include="Views\Suppliers\" />
    <Folder Include="Views\Products\" />
  </ItemGroup>
